<body class="layout-boxed">

    <style>
        #Loading {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0,0,0,0.6);
            z-index: 999999;
        }

        .page-spinner-bar {
            position: fixed;
            z-index: 10051;
            width: 100px;
            top: 40%;
            left: 50%;
            margin-left: -55px;
            text-align: center;
        }

        .fixed-table-loading {
            visibility: hidden;
            top: 0px;
            display: none;
        }

        .loading-wrap {
            visibility: hidden;
            display: none;
        }

        .table-loading {
            visibility: hidden;
            display: none;
        }

        .loading {
            visibility: hidden;
            display: none;
        }
    </style>
    <div id="load_screen">
        <div class="loader">
            <div class="loader-content">
                <div class="spinner-grow align-self-center"></div>
            </div>
        </div>
    </div>
    <div id="Loading" style="display:none;">
        <div class="page-spinner-bar ">
            <div class="spinner-grow text-success align-self-center"></div>
            <div class="spinner-grow text-info align-self-center"></div>
            Lütfen Bekleyiniz...
        </div>

    </div>

